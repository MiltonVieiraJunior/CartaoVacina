<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.neri.alexa.cartaovacina.DescricaoHospitalActivity">

    <TextView
        android:id="@+id/desHospital"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textViewDescFone"
        android:layout_marginStart="47dp"
        android:layout_marginTop="2dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="14dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewDescFone"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/desHospital"
        android:layout_marginTop="48dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/desHospital"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewDesBairro"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textViewDesendereco"
        android:layout_marginEnd="4dp"
        app:layout_constraintRight_toRightOf="@+id/textViewDesendereco"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewDescFone"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toLeftOf="@+id/textViewDescFone" />

    <TextView
        android:id="@+id/textViewDesendereco"
        android:layout_width="112dp"
        android:layout_height="35dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="47dp"
        android:layout_marginTop="158dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
